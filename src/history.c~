List *init_history()
{
  List *historyList = malloc(sizeof(HistoryList));
  historyList ->root = malloc(sizeof(Item));

  return historyList;
}

void add_history(List *list,char *str)
{
  Item *current = list->root;
  Item *newNode = (Item*) malloc(sizeof(Item));
  newNode->next = NULL;

  while (newNode-> != NULL)
    {
      newNode = newNode->next;
    }
